			<py:if test="rsc_dict.get('format')">
      <py:choose>
        <py:when test="rsc_dict.get('format').lower() == 'csv'">
				<dct:format><dct:IMT rdf:value="text/csv" rdfs:label="${rsc_dict.get('format')}" /></dct:format><dcat:mediaType>text/csv</dcat:mediaType>
        </py:when>
