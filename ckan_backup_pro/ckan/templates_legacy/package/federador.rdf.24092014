			</py:if>
			<dcat:accessURL  rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">${c.urlenco(unicode((rsc_dict['url'].split("?"))[0]+  '.ttl?api_key=e103dc13eb276ad734e680f5855f20c6&amp;_view=completa').encode('utf8'), ':?=/&amp;%') }</dcat:accessURL>
			<dct:format><dct:IMT rdf:value="text/turtle" rdfs:label="RDF-Turtle" /></dct:format><dcat:mediaType>text/turtle</dcat:mediaType>
			</dcat:Distribution>
		</dcat:distribution>
	   </py:if>
	   <py:if test="not('/recurso/territorio/' in rsc_dict['url'])">
		<dcat:distribution>
		  <dcat:Distribution>
			<dct:identifier>${'http://opendata.aragon.es/data/'+ item.get('name') + '/' + rsc_dict.get('format').lower()}</dct:identifier>
			<py:if test="rsc_dict.get('description')">
			   <dct:title xml:lang="es">${rsc_dict.get('description')}</dct:title>
			</py:if>
